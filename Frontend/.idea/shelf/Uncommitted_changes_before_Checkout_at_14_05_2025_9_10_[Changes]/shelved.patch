<+><?xml version=\"1.0\" encoding=\"UTF-8\"?>\r\n<configuration>\r\n\r\n    <!-- Appender que guarda los logs en archivo -->\r\n    <appender name=\"FILE\" class=\"ch.qos.logback.core.rolling.RollingFileAppender\">\r\n        <!-- Política de rotación por día -->\r\n        <rollingPolicy class=\"ch.qos.logback.core.rolling.TimeBasedRollingPolicy\">\r\n            <fileNamePattern>logs/entrebicisBackendWeb.%d{yyyy-MM-dd}.log</fileNamePattern>\r\n            <maxHistory>30</maxHistory>\r\n        </rollingPolicy>\r\n\r\n        <encoder>\r\n            <pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>\r\n        </encoder>\r\n    </appender>\r\n\r\n    <appender name=\"API_FILE\" class=\"ch.qos.logback.core.rolling.RollingFileAppender\">\r\n        <rollingPolicy class=\"ch.qos.logback.core.rolling.TimeBasedRollingPolicy\">\r\n            <fileNamePattern>logs/entrebicisBackendApi.%d{yyyy-MM-dd}.log</fileNamePattern>\r\n            <maxHistory>30</maxHistory>\r\n        </rollingPolicy>\r\n\r\n        <encoder>\r\n            <pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>\r\n        </encoder>\r\n    </appender>\r\n\r\n    <logger name=\"cat.copernic.backend.controllers.api\" level=\"INFO\" additivity=\"false\">\r\n        <appender-ref ref=\"API_FILE\" />\r\n    </logger>\r\n\r\n    <!-- Niveles de log global -->\r\n    <root level=\"INFO\">\r\n        <appender-ref ref=\"FILE\" />\r\n    </root>\r\n\r\n</configuration>\r\n\r\n
===================================================================
